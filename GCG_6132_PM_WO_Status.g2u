Program.Sub.LoadDtDvWOs.Start
	V.Local.iCnt.Declare()
	V.Local.iProgress.Declare()
	V.Local.sSQL.Declare()
	V.Local.sRet.Declare()
	V.Local.sJob.Declare()
	V.Local.sSuffix.Declare()
	V.Local.sPrintedAndReleased.Declare()
	V.Local.fEstHrs.Declare()
	V.Local.fRemHrs.Declare()
	V.Local.fPcsRem.Declare()
	V.Local.sOper.Declare()
	V.Local.bDictExists.Declare()
	v.Local.sFilter.Declare()

	F.Intrinsic.Control.Try
	
		F.Intrinsic.Control.If(V.DataTable.dtWOs.Exists, =, True)
			F.Data.DataTable.Close("dtWOs")
		F.Intrinsic.Control.EndIf
	
		F.Data.Dictionary.Exists("dIndirect", V.Local.bDictExists)
		F.Intrinsic.Control.If(V.Local.bDictExists, =, True)
			F.Data.Dictionary.Close("dIndirect")
		F.Intrinsic.Control.EndIf
		
		V.Global.bLoadData.Set(True)
		
		F.Intrinsic.Control.If(V.Caller.GSSVersion, =, "2016.1", OR, V.Caller.GSSVersion, =, "2016.2")
		
			V.Local.sPrintedAndReleased.Set("CASE When FLAG_WO_PRTD = 'Y' OR FLAG_WO_PRTD = 'R' THEN CONVERT(1,SQL_BIT) WHEN FLAG_WO_PRTD !='Y' Or Flag_WO_PRTD != 'R' Then convert(0,SQL_BIT) END AS PRINTED, CONVERT(CASE WHEN FLAG_WO_PRTD = 'Y' THEN 0 WHEN FLAG_WO_PRTD = 'X' THEN 1 WHEN FLAG_WO_PRTD = '' THEN 0 WHEN FLAG_WO_PRTD = 'R' THEN 1 END,SQL_BIT) AS RELEASED")
			
		F.Intrinsic.Control.Else
		
			V.Local.sPrintedAndReleased.Set("CONVERT(CASE WHEN FLAG_WO_PRTD = 'Y' THEN 1 WHEN FLAG_WO_PRTD = 'X' THEN 0 WHEN FLAG_WO_PRTD = '' THEN 0 WHEN FLAG_WO_PRTD = 'R' THEN 1 END,SQL_BIT) AS PRINTED, CASE WHEN A.FLAG_WO_RELEASED ='Y' THEN CONVERT(1,SQL_BIT) ELSE CONVERT (0, SQL_BIT) END AS RELEASED")
			
		F.Intrinsic.Control.EndIf
		
		F.Intrinsic.String.Build("SELECT '' AS Plus, {0}, CASE WHEN A.FLAG_HOLD = 'Y' THEN CONVERT(1,SQL_BIT) WHEN A.FLAG_HOLD = 'N' THEN CONVERT(0,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END AS FLAG_HOLD,A.JOB, A.SUFFIX,isNumeric(a.suffix) as Numero,CONCAT(a.job,A.suffix) as JobKey, A.PROJECT, PHASE, DESCR, NAME_CUSTOMER,a.customer_po,concat(a.Sales_order,a.Sales_order_line) as SO_LINE,concat(a.Sales_order,Concat('-',a.Sales_order_line)) as O_LINE,'' As SO_NOTES, COALESCE(PART,'') AS PART, '' As DISPLAY_PART, LOCATION,CAST(1 AS bit) AS XPROC,CAST(1 AS bit) AS OPROC, PRODUCT_LINE, DESCRIPTION, DATE_START,date_due as Parent_date, DATE_DUE, CONVERT(0,SQL_FLOAT) AS EST_HRS, NUM_HOURS AS ACT_HRS, CONVERT(0,SQL_FLOAT) AS REM_HRS, '' AS OPER, QTY_ORDER, QTY_ORDER - QTY_COMPLETED AS PCS_REM, C.NOTES, CONCAT(A.JOB,A.SUFFIX) AS JS, CONVERT(0,SQL_BIT) AS IND_FLAG FROM V_JOB_HEADER A LEFT JOIN V_CUSTOMER_MASTER B ON A.CUSTOMER = B.CUSTOMER LEFT JOIN GCG_6132_WO_NOTES C ON A.JOB = C.JOB AND A.SUFFIX = C.SUFFIX LEFT JOIN PROJECT_MASTER D ON A.PROJECT = D.PROJECT WHERE A.DATE_CLOSED = '1900-1-1' ORDER BY DATE_DUE, A.JOB, A.SUFFIX;", V.Local.sPrintedAndReleased, V.Local.sSql)
		F.Data.DataTable.CreateFromSQL("dtWOs", "con", V.Local.sSQL, True)
	
		F.Data.DataTable.AddDisplayPartColumn("dtWOs", 0, "PART", "DISPLAY_PART")
	
		Gui.F_WO.lblStatus.Visible(True)
		Gui.F_WO.progressBarStatus.Visible(True)
		Gui.F_WO.lblStatus.Caption("Loading Work Orders....")
		Gui.F_WO.progressBarStatus.Min(0)
		Gui.F_WO.progressBarStatus.Max(V.DataTable.dtWOs.RowCount)
		
		
		f.Data.Datatable.CreateFromSQL("DtAPSOpen","con","SELECT JSS,Job,sfx,SEQTYPE, SEQRES, PCSREMAIN,'' as LMO FROM APSV3_JBLINES WHERE COMPLDATE IS NULL ORDER BY JOB,SFX,SEQ;")
		f.Data.DataView.create("DtAPSOpen","DtApsOpenVw",22,"","JOB ASC, SFX ASC")
		
		f.Data.Dictionary.CreateFromSQL("JobOpsLMODict","con","select Job + suffix + Seq,LMO from V_Job_operations")
		f.Data.Dictionary.SetDefaultReturn("JobOpsLMODict","")
		f.Data.Datatable.FillFromDictionary("DtAPSOpen","JobOpsLMODict","JSS","LMO")
		f.Data.Dictionary.Close("JobOpsLMODict")
		
		
		
		f.Data.Datatable.CreateFromSQL("DtAPSEstHrs","con","SELECT job,sfx,CONVERT(SUM(HOURSORIG + INITSU), SQL_NUMERIC) AS EST_HRS FROM APSV3_JBLINES WHERE SEQTYPE = '1' GROUP BY JOB,SFX;")
		f.Data.DataView.create("DtAPSEstHrs","DtAPSEstHrsVw",22,"","JOB ASC, SFX ASC")
		
		
		F.Intrinsic.Control.For(V.Local.iCnt, 0, V.DataTable.dtWOs.RowCount--, 1)
		
			F.Intrinsic.Math.Add(V.Local.iProgress, 1, V.Local.iProgress)
			Gui.F_WO.progressBarStatus.Value(V.Local.iProgress)
			
			V.Local.sJob.Set(V.DataTable.dtWOs(V.Local.iCnt).JOB!FieldVal)
			V.Local.sSuffix.Set(V.DataTable.dtWOs(V.Local.iCnt).SUFFIX!FieldVal)
			
			f.Intrinsic.String.Build("Job = '{0}' and sfx = '{1}' and LMO <> 'M'", v.Local.sJob,v.Local.sSuffix,v.Local.sFilter)
			f.Data.DataView.SetFilter("DtAPSOpen","DtApsOpenVw",v.Local.sFilter)
			F.Intrinsic.Control.If(v.DataView.DtAPSOpen!DtApsOpenVw.RowCount, =, 0)
				V.Local.sOper.Set("WipToFG")
				V.Local.FPcsRem.Set(0)
			F.Intrinsic.Control.Else
				V.Local.sOper.Set(v.DataView.DtAPSOpen!DtApsOpenVw(0).SEQRES!FieldValTrim)
				V.Local.fPcsRem.Set(v.DataView.DtAPSOpen!DtApsOpenVw(0).PCSREMAIN!FieldValTrim)
			F.Intrinsic.Control.EndIf
			
			f.Intrinsic.String.Build("Job = '{0}' and sfx = '{1}'", v.Local.sJob,v.Local.sSuffix,v.Local.sFilter)
			f.Data.DataView.SetFilter("DtAPSEstHrs","DtAPSEstHrsVw",v.Local.sFilter)
			f.Intrinsic.Control.If(v.DataView.DtAPSEstHrs!DtAPSEstHrsVw.RowCount,>,0)
				v.Local.fEstHrs.Set(v.DataView.DtAPSEstHrs!DtAPSEstHrsVw(0).EST_HRS!FieldValFloat)
			f.Intrinsic.Control.Else
				v.Local.fEstHrs.Set(0)
			f.Intrinsic.Control.EndIf
			
'			F.Intrinsic.String.Build("SELECT TOP 1 a.SEQTYPE, a.SEQRES, a.PCSREMAIN FROM APSV3_JBLINES a inner join V_job_operations b on a.JSS = b.job + b.suffix + b.seq WHERE a.COMPLDATE IS NULL AND a.JOB = '{0}' AND a.SFX = '{1}' AND b.LMO <> 'M'  ORDER BY a.JOB,a.SFX,a.SEQ;", V.Local.sJob, V.Local.sSuffix, V.Local.sSQL)
'			F.ODBC.Connection!con.OpenLocalRecordsetRO("rstInfo", V.Local.sSQL)
'				F.Intrinsic.Control.If(V.ODBC.con!rstInfo.EOF, =, True)
'					V.Local.sOper.Set("WipToFG")
'					V.Local.FPcsRem.Set(0)
'				F.Intrinsic.Control.Else
'					V.Local.sOper.Set(V.ODBC.con!rstInfo.FieldValTrim!SEQRES)
'					V.Local.fPcsRem.Set(V.ODBC.con!rstInfo.FieldValFloat!PCSREMAIN)
'				F.Intrinsic.Control.EndIf
'			F.ODBC.con!rstInfo.Close
'	
'			F.Intrinsic.String.Build("SELECT CONVERT(SUM(HOURSORIG + INITSU), SQL_NUMERIC) AS EST_HRS FROM APSV3_JBLINES WHERE SEQTYPE = '1' AND JOB = '{0}' AND SFX = '{1}' GROUP BY JOB,SFX;",V.Local.sJob,V.Local.sSuffix,V.Local.sSQL)
'			F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.fEstHrs)
		
			F.Intrinsic.Control.If(V.Local.fEstHrs, =, 0)
				V.Local.fEstHrs.Set(0.0000)
				V.Local.fRemHrs.Set(0.0000)
			F.Intrinsic.Control.ElseIf(V.DataTable.dtWOs(V.Local.iCnt).ACT_HRS!FieldValFloat, >, V.Local.fEstHrs)
				V.Local.fRemHrs.Set(0.0000)
			F.Intrinsic.Control.Else
				F.Intrinsic.Math.Sub(V.Local.fEstHrs, V.DataTable.dtWOs(V.Local.iCnt).ACT_HRS!FieldValFloat, V.Local.fRemHrs)
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.Control.If(V.Local.sOper, =, "WipToFG")
				V.Local.fRemHrs.Set(0.0000)
			F.Intrinsic.Control.EndIf
	
			F.Data.DataTable.SetValue("dtWOs", V.Local.iCnt, "EST_HRS", V.Local.fEstHrs, "REM_HRS", V.Local.fRemHrs, "OPER", V.Local.sOper)

		F.Intrinsic.Control.Next(V.Local.iCnt)
		f.Data.Datatable.Close("DtAPSOpen")
		f.Data.Datatable.Close("DtAPSEstHrs")
		
		
		' If a row's JS was found in IND_LBR_ACCTS, then remove it for being Indirect.
		F.Data.Dictionary.CreateFromSQL("dIndirect", "con", "SELECT DISTINCT CONCAT(job, suffix) AS JS, CONVERT(IF('' = '', 1 ,0), SQL_BIT) FROM ind_lbr_accts;")
		F.Data.Dictionary.SetDefaultReturn("dIndirect", False)
		F.Data.DataTable.FillFromDictionary("dtWOs", "dIndirect", "JS", "IND_FLAG")
		
		
		'f.Data.DataView.Create("dtWOs","DV_ParentDue",22,"Suffix = '000' or Numero = 0","job asc,Date_due asc")
		f.Data.DataView.Create("dtWOs","DV_ParentDue",22,"Suffix = '000'","job asc,Date_due asc")
		f.Data.Dictionary.CreateFromDataView("ParentDateDict","DtWos","DV_ParentDue","JOB","DATE_DUE")
		f.Data.DataView.Close("dtWOs","DV_ParentDue")
		f.Data.Dictionary.SetDefaultReturn("ParentDateDict","1900-01-01")
		
		f.Data.Datatable.FillFromDictionary("dtWOs","ParentDateDict","JOB","Parent_date")
		f.Data.Dictionary.Close("ParentDateDict")
		
		F.Data.DataView.Create("dtWOs", "dvWOs" ,22, "IND_FLAG <> True", "Parent_date asc, JOB ASC,DATE_DUE ASC, SUFFIX ASC")
		
		
		
		
		
		f.Data.Dictionary.CreateFromSQL("XprocDict","Con","Select distinct CONCAT(J1.JOB,'000') AS JOBKEY, CAST(1 AS bit) AS XPROC from V_JOB_OPERATIONS J1 inner Join V_Job_header J2 on j1.job = j2.job and j1.suffix = j2.suffix  WHERE J1.LMO = 'L' and J1.PART IN ('1TRK','2TRK') and J2.DATE_CLOSED = '1900-1-1' union Select distinct CONCAT(J1.JOB,J1.Suffix) AS JOBKEY, CAST(1 AS bit) AS XPROC from V_JOB_OPERATIONS J1 inner Join V_Job_header J2 on j1.job = j2.job and j1.suffix = j2.suffix  WHERE J1.LMO = 'L' and J1.PART IN ('1TRK','2TRK') and J2.DATE_CLOSED = '1900-1-1'")
		f.Data.Dictionary.SetDefaultReturn("XprocDict","False")
		f.Data.Datatable.FillFromDictionary("dtWOs","XprocDict","JobKey","XPROC")
		f.Data.Dictionary.Close("XprocDict")
		
		f.Data.Dictionary.CreateFromSQL("OprocDict","Con","Select distinct CONCAT(J1.JOB,'000') AS JOBKEY, CAST(1 AS bit) AS OPROC from V_JOB_OPERATIONS J1 inner Join V_Job_header J2 on j1.job = j2.job and j1.suffix = j2.suffix  WHERE J1.LMO = 'O' and J2.DATE_CLOSED = '1900-1-1' and J1.SEQ < 995000 Union Select distinct CONCAT(J1.JOB,J1.Suffix) AS JOBKEY, CAST(1 AS bit) AS OPROC from V_JOB_OPERATIONS J1 inner Join V_Job_header J2 on j1.job = j2.job and j1.suffix = j2.suffix  WHERE J1.LMO = 'O' and J2.DATE_CLOSED = '1900-1-1' and J1.SEQ < 995000")
		f.Data.Dictionary.SetDefaultReturn("OprocDict","False")
		f.Data.Datatable.FillFromDictionary("dtWOs","OprocDict","JobKey","OPROC")
		f.Data.Dictionary.Close("OprocDict")
		
'		f.Data.DataTable.AddExpressionColumn("dtWOs","ProcLoc","String","IIF(LOCATION = '02' and XPROC = false,'02',IIF(Location = '02' and XPROC = true,'S to N',IIF(Location = '01' and XPROC = False,'01',IIF(Location = '01' and XPROC = true,'N to S',''))))")
		f.Data.DataTable.AddExpressionColumn("dtWOs","ProcLoc","String","IIF(LOCATION = '02' and XPROC = false,'02',IIF(Location = '02' and XPROC = true,'N to S',IIF(Location = '01' and XPROC = False,'01',IIF(Location = '01' and XPROC = true,'S to N',''))))")
		f.Intrinsic.Control.If(v.DataTable.dtOutProc.Exists)
			f.Data.Datatable.Close("dtOutProc")
		f.Intrinsic.Control.EndIf
		f.Data.Datatable.CreateFromSQL("dtOutProc","Con","Select j1.Job, j1.Suffix, j1.Seq,J1.Description,J1.Date_Start,j1.Flag_Closed from V_JOB_OPERATIONS J1 inner Join V_Job_header J2 on j1.job = j2.job and j1.suffix = j2.suffix  WHERE J1.LMO = 'O' and J2.DATE_CLOSED = '1900-1-1' and J1.SEQ < 995000 order by j1.Job,j1.suffix", True)
		
		'CUSTOMER NOTES fro ARC 4167
		F.Data.Dictionary.CreateFromSQL("dictNotes","con","Select Cast(Concat(SO,SOLINE) as CHAR(10)),RTrim(Notes) as Notes From GAB_4167_SO_NOTES;")
		F.Data.Dictionary.SetDefaultReturn("dictNotes","")
		F.Data.DataTable.FillFromDictionary("DtWos","dictNotes","SO_LINE","SO_NOTES")
		F.Data.Dictionary.Close("dictNotes")
		
		f.Data.DataView.Create("dtOutProc","dtOutProcVw",22)
		f.Intrinsic.Control.CallSub(LoadGVOutProc)
	F.Intrinsic.Control.Catch
	
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}", V.Ambient.ScriptPath, V.Ambient.Newline, V.Ambient.CurrentSubroutine, V.Ambient.ErrorNumber, V.Ambient.ErrorDescription, V.Ambient.GABVersion, V.Global.sError)
		F.Intrinsic.UI.Msgbox(V.Global.sError)
		F.Intrinsic.Control.End()
		
	F.Intrinsic.Control.EndTry
	
Program.Sub.LoadDtDvWOs.End

