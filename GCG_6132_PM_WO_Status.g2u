Program.Sub.GsGCWO_CellValueChanged.Start
	V.Local.sSQL.Declare()
	V.Local.sJob.Declare()
	V.Local.sSuffix.Declare()
	V.Local.sText.Declare()
	v.Local.sTextTbl.Declare
	v.Local.sSOline.Declare
	v.Local.sSO.Declare
	v.Local.sLine.Declare

	F.Intrinsic.Control.Try

		F.Intrinsic.Control.If(V.Args.COLUMN, =, "NOTES")
			
			Gui.F_WO.GsGCWO.GetCellValueByColumnName("gvWOs", "JOB", V.Args.RowIndex, V.Local.sJob)
			Gui.F_WO.GsGCWO.GetCellValueByColumnName("gvWOs", "SUFFIX", V.Args.RowIndex, V.Local.sSuffix)
			F.Intrinsic.String.Replace(V.Args.Value, "'", "''", V.Local.sText)
			
			' If the JS already exists in the notes, then Delete first before Insert.
			F.Intrinsic.String.Build("SELECT * FROM GCG_6132_WO_NOTES WHERE js = '{0}{1}';", V.Local.sJob, V.Local.sSuffix, V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRW("rstNotes",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstNotes.EOF, =, False)
			
				F.Intrinsic.String.Build("DELETE FROM GCG_6132_WO_NOTES WHERE js = '{0}{1}';", V.Local.sJob, V.Local.sSuffix, V.Local.sSQL)
				F.ODBC.Connection!con.Execute(V.Local.sSQL)
				
			F.Intrinsic.Control.EndIf
			
			F.Intrinsic.String.Build("INSERT INTO GCG_6132_WO_NOTES(js, job, suffix, notes) VALUES('{0}{1}','{0}','{1}','{2}');", V.Local.sJob, V.Local.sSuffix, V.Local.sText, V.Local.sSQL)
			F.ODBC.Connection!con.Execute(V.Local.sSQL)
			
			F.ODBC.con!rstNotes.Close()
		f.Intrinsic.Control.ElseIf(V.Args.COLUMN, =, "SO_NOTES")
			Gui.F_WO.GsGCWO.GetCellValueByColumnName("gvWOs", "SO_LINE", V.Args.RowIndex, V.Local.sSOline)
			f.Intrinsic.Control.If(v.Local.sSOline.Trim,=,"")
				f.Intrinsic.Control.ExitSub
			f.Intrinsic.Control.EndIf
			
			F.Intrinsic.String.Replace(V.Args.Value,"'","''",V.Local.sTextTbl)
			F.Intrinsic.String.Build("SELECT * FROM GAB_4167_SO_NOTES WHERE SO + SOLINE = '{0}';",V.Local.sSOLine,V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRW("rstNotes",V.Local.sSQL)
			f.Intrinsic.String.Left(v.Local.sSOline,7,v.Local.sSO)
			f.Intrinsic.String.Right(v.Local.sSOline,3,v.Local.sLine)
			f.Intrinsic.Control.If(v.Local.sSO.Trim,=,"0000000","OR",v.Local.sSO.Trim,=,"")
				f.Intrinsic.Control.ExitSub
			f.Intrinsic.Control.EndIf
			F.Intrinsic.Control.If(V.ODBC.con!rstNotes.EOF,=,True)
				
				F.Intrinsic.String.Build("INSERT INTO GAB_4167_SO_NOTES(SO,SOLINE,NOTES) VALUES('{0}','{1}','{2}');",V.Local.sSO,V.Local.sLine,V.Local.sTextTbl,V.Local.sSQL)
				F.ODBC.Connection!con.Execute(V.Local.sSQL)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("UPDATE GAB_4167_SO_NOTES SET NOTES = '{0}' WHERE SO = '{1}' AND SOLINE = '{2}';",V.Local.sTextTbl,V.Local.sSO,V.Local.sLine,V.Local.sSQL)
				F.ODBC.Connection!con.Execute(V.Local.sSQL)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstNotes.Close
		F.Intrinsic.Control.EndIf
	
	F.Intrinsic.Control.Catch
	
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}", V.Ambient.ScriptPath, V.Ambient.Newline, V.Ambient.CurrentSubroutine, V.Ambient.ErrorNumber, V.Ambient.ErrorDescription, V.Ambient.GABVersion, V.Global.sError)
		F.Intrinsic.UI.Msgbox(V.Global.sError)
		F.Intrinsic.Control.End()
		
	F.Intrinsic.Control.EndTry
	
Program.Sub.GsGCWO_CellValueChanged.End

